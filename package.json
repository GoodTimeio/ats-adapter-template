{
    "name": "goodtime-ats-adapter-template",
    "version": "0.0.0",
    "description": "This is a boilerplate project for GoodTime ATS Adapters.",
    "main": "src/main.ts",
    "engines": {
        "node": "12.15.0",
        "npm": "6.12.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/GoodTimeio/ats-adapter-template"
    },
    "keywords": [
        "boilerplate",
        "node",
        "goodtime",
        "ats"
    ],
    "author": "Goodtime Engineering",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/GoodTimeio/ats-adapter-template/issues"
    },
    "homepage": "https://github.com/GoodTimeio/ats-adapter-template#readme",
    "dependencies": {
        "@goodtimeio/node-logger": "1.8.0",
        "@nestjs/common": "7.3.2",
        "@nestjs/core": "7.3.2",
        "@nestjs/passport": "7.1.0",
        "@nestjs/platform-express": "7.3.2",
        "cls-hooked": "4.2.2",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "npm-run-all": "4.1.5",
        "passport": "0.4.1",
        "passport-local": "1.0.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.3",
        "source-map-support": "0.5.19",
        "ts-node": "8.6.2",
        "typeorm": "0.2.28"
    },
    "devDependencies": {
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@nestjs/testing": "7.4.4",
        "@semantic-release/changelog": "3.0.6",
        "@semantic-release/commit-analyzer": "7.0.0",
        "@semantic-release/git": "8.0.0",
        "@semantic-release/github": "6.0.1",
        "@semantic-release/npm": "6.0.0",
        "@semantic-release/release-notes-generator": "7.3.5",
        "@types/cls-hooked": "4.3.1",
        "@types/express": "4.17.3",
        "@types/jest": "26.0.14",
        "@types/node": "13.1.8",
        "@types/passport-local": "1.0.33",
        "@types/uuid": "3.4.6",
        "@typescript-eslint/eslint-plugin": "2.16.0",
        "@typescript-eslint/parser": "2.16.0",
        "commitizen": "4.2.1",
        "cz-conventional-changelog": "3.0.2",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.9.0",
        "eslint-plugin-prettier": "3.1.2",
        "expect": "24.9.0",
        "husky": "4.0.10",
        "jest": "26.4.2",
        "lint-staged": "10.4.2",
        "marked": "0.7.0",
        "nodemon": "2.0.4",
        "prettier": "1.19.1",
        "prettier-eslint": "9.0.1",
        "semantic-release": "16.0.2",
        "ts-jest": "26.4.1",
        "tsconfig-paths": "3.9.0",
        "tscpaths": "0.0.9",
        "typescript": "4.0.2"
    },
    "scripts": {
        "dev": "nodemon --inspect=0.0.0.0",
        "dev:debug": "node --inspect=0.0.0.0 --require ts-node/register --require tsconfig-paths/register src/main.ts",
        "prod": "node -r source-map-support/register dist/src/main.js",
        "build": "run-s build:compile build:fix-paths",
        "build:ci": "run-s build:clean build",
        "build:clean": "rm -rf ./dist",
        "build:compile": "tsc",
        "build:fix-paths": "tscpaths -p tsconfig.json -s . -o ./dist",
        "pretest": "npm run lint",
        "lint": "tsc && eslint --quiet . --ext .ts,.js --fix",
        "test": "jest -V --coverage",
        "prettier": "prettier src/**/*.ts",
        "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
        "semantic-release": "semantic-release",
        "cm": "git-cz"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "*.{ts,js}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsc && lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
